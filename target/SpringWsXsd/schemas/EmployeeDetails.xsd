<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://spring.ws.com"
	targetNamespace="http://spring.ws.com" elementFormDefault="qualified"
	attributeFormDefault="unqualified">
	<xs:element name="EmployeeRequest">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="name" type="xs:string" minOccurs="0" />
				<xs:element name="designation" type="xs:string"
					minOccurs="0" />
			</xs:sequence>
			<xs:attribute name="userId" type="xs:int" use="required" />
		</xs:complexType>
	</xs:element>

	<xs:element name="EmployeeResponse">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="name" type="xs:string" minOccurs="0" />
				<xs:element name="designation" type="xs:string"
					minOccurs="0" />
			</xs:sequence>
		</xs:complexType>
	</xs:element>

</xs:schema>